>then : <U>(callback: (x: T) => IPromise<U>) => IPromise<U>
>U : U
>callback : (x: T) => IPromise<U>
>x : T
>T : T
>IPromise : IPromise<T>
>U : U
>IPromise : IPromise<T>
>U : U
}
interface Promise<T> {
>Promise : Promise<T>
>T : T

    then<U>(callback: (x: T) => Promise<U>): Promise<U>;
>then : <U>(callback: (x: T) => Promise<U>) => Promise<U>
>U : U
>callback : (x: T) => Promise<U>
>x : T
