>F : F
>E : E

    new (a: string): string;
>a : string
}

var d: D;
>d : D
>D : D

d(""); // string
>d("") : string
>d : D
>"" : ""

new d(""); // should be number
>new d("") : number
>d : D
>"" : ""

var f: F;
>f : F
>F : F

new f(""); // string
>new f("") : string
