// merged interfaces behave as if all extends clauses from each declaration are merged together
// no errors expected

class C<T> {
>C : C<T>
>T : T

    a: T;
>a : T
>T : T
}

class C2<T> {
>C2 : C2<T>
>T : T

    b: T;
>b : T
>T : T
}

class C3<T> {
>C3 : C3<T>
>T : T

    c: T;
>c : T
>T : T
}

class C4<T> {
>C4 : C4<T>
>T : T

    d: T;
>d : T
>T : T
}

interface A<T> extends C<string>, C3<string> {
>A : A<T>
>T : T
>C : C<T>
>C3 : C3<T>

    y: T;
>y : T
>T : T
}

interface A<T> extends C<string>, C4<string> {
>A : A<T>
>T : T
>C : C<T>
>C4 : C4<T>

    z: T;
>z : T
>T : T
}

class D implements A<boolean> {
>D : D
>A : A<T>

    a: string;
>a : string

    b: Date;
>b : Date
>Date : Date

    c: string;
>c : string

    d: string;
>d : string

    y: boolean;
>y : boolean

    z: boolean;
