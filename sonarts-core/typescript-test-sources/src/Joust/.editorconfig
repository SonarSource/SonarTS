# EditorConfig: http://editorconfig.org
root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true

[*.{js,ts,tsx,less}]
indent_style = tab
indent_size = 4
trim_trailing_whitespace = true

[*.json]
indent_style = tab
indent_size = 4

[package.json]
indent_style = space
indent_size = 2
