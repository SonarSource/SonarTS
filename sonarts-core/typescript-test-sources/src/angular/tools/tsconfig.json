{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "../dist/tools/",
    "noImplicitAny": true,
    "noFallthroughCasesInSwitch": true,
    "paths": {
    },
    "rootDir": ".",
    "sourceMap": true,
    "inlineSources": true,
    "lib": ["es6", "dom"],
    "target": "es5",
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "typings-test",
    "public_api_guard",
    "docs"
  ]
}
