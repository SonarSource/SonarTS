{
  "extends": "../tsconfig-build",

  "compilerOptions": {
    "paths": {
      "@angular/animations": ["../../dist/packages/animations"]
    }
  },
  "files": [
    "public_api.ts",
    "../../../node_modules/zone.js/dist/zone.js.d.ts",
