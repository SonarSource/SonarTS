[
  {
    "seq": 0,
    "type": "request",
    "command": "configure",
    "arguments": {
      "hostInfo": "vscode"
    }
  },
  {
    "seq": 1,
    "type": "request",
    "command": "compilerOptionsForInferredProjects",
    "arguments": {
      "options": {
        "module": "CommonJS",
        "target": "ES6",
        "allowSyntheticDefaultImports": true,
        "allowNonTsExtensions": true,
        "allowJs": true,
        "jsx": "Preserve"
      }
    }
  },
  {
    "seq": 2,
    "type": "request",
    "command": "open",
    "arguments": {
      "file": "$$PWD$$/app/app.module.ts",
      "fileContent": ""
    }
  },
  {
    "seq": 3,
    "type": "request",
    "command": "geterr",
    "arguments": {
      "delay": 0,
      "files": [
        "$$PWD$$/app/app.module.ts"
      ]
    }
  }
]