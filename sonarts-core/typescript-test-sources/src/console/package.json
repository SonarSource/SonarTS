{
  "private": true,
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production GRAPHQL_ENDPOINT=$BACKEND_ADDR/system webpack --config webpack.config.build.js && cp -r static/* dist",
    "deploy": "npm run dll && npm run build && cp dll/vendor.bundle.js dist/vendor.bundle.js && cp dist/index.html dist/200.html && netlify deploy -p dist -s $NETLIFY_ID -t $NETLIFY_TOKEN",
    "lint": "tslint \"src/**/*.ts{,x}\"",
    "start": "GRAPHQL_ENDPOINT=$BACKEND_ADDR/system webpack-dev-server --hot --inline --progress --history-api-fallback --host 0.0.0.0 --port 4000 --content-base dll/",
    "test": "npm run lint && jest",
    "test-quick": "jest",
    "test-watch": "jest --watch",
    "dll": "webpack --config dll.webpack.config.js"
  },
  "pre-push": [
    "test-quick"
  ],
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "tsx",
      "json"
    ],
    "mocksPattern": ".*src.*(?:[\\/]|^)__mocks__[\\/]",
    "testPathIgnorePatterns": [
      "/lib/"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!**/__tests__/**"
    ],
    "setupFiles": [
      "<rootDir>/jest-setup.js",
      "<rootDir>/src/utils/polyfils.ts"
    ]
  },
