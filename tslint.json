{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [true, "never-prefix"],
        "member-ordering": [false],
        "no-console": [false],
        "no-bitwise": false,
        "arrow-parens": [false],
        "max-classes-per-file": [false]
    },
    "rulesDirectory": ["lib/rules"]
}